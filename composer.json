{
    "name": "bread/module-breadcheckout",
    "description": "Offers the Bread financing and checkout tools for your Magento store",
    "license": "MIT",
    "minimum-stability": "stable",
    "version": "1.1.0",
    "require": {
        "php": "~7.1.3||~7.2.0",
        "magento/module-config": "101.1.*",
        "magento/module-store": "101.0.*",
        "magento/module-checkout": "100.3.*",
        "magento/module-sales": "102.0.*",
        "magento/module-quote": "101.1.*",
        "magento/module-customer": "102.0.*",
        "magento/module-payment": "100.3.*",
        "magento/module-backend": "101.0.*",
        "magento/module-tax": "100.3.*",
        "magento/module-directory": "100.3.*",
        "magento/module-theme": "101.0.*",
        "magento/module-catalog": "103.0.*",
        "magento/module-eav": "102.0.*",
        "magento/framework": "102.0.*",
        "magento/module-ui": "101.1.*",
        "magento/module-vault": "101.1.*",
        "lib-libxml": "*"
    },
    "suggest": {
        "magento/module-checkout-agreements": "100.3.*"
    },
    "type": "magento2-module",
    "repositories": [{
